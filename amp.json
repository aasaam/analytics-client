{
  "requests": {
    "pageView": "__COLLECTOR_URL__/?m=pv_amp&i=${i}"
  },
  "extraUrlParams": {
    "cid_amp": "${clientId(aa_amp_std)}",
    "p": {
      "u": "${ampdocUrl}",
      "t": "${t}",
      "l": "${l}",
      "cu": "${canonicalUrl}",
      "ei": "${ei}",
      "em": "${em}",
      "et": "${et}",
      "r": "${documentReferrer}",
      "scr": "${screenWidth}x${screenHeight}",
      "vps": "${viewportWidth}x${viewportHeight}",
      "cd": "${screenColorDepth}",
      "k": "${k}",
      "usr": "${usr}",
      "prf": {
        "plt": "${pageLoadTime}",
        "dlt": "${domainLookupTime}",
        "tct": "${tcpConnectTime}",
        "srt": "${serverResponseTime}",
        "pdt": "${pageDownloadTime}",
        "rt": "${redirectTime}",
        "dit": "${domInteractiveTime}",
        "clt": "${contentLoadTime}"
      }
    }
  },
  "triggers": {
    "pageView": {
      "on": "visible",
      "request": "pageView"
    }
  },
  "transport": {
    "beacon": true,
    "useBody": true
  }
}
