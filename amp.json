{
  "requests": {
    "pageView": "__COLLECTOR_URL__/?m=amp&i=${i}&u=${ampdocUrl}&r=${documentReferrer}&cn=${canonicalUrl}&mid=${mid}"
  },
  "extraUrlParams": {
    "c": "${clientId(cid-aai)}",
    "p": {
      "scr": "${screenWidth}x${screenHeight}",
      "vps": "${viewportWidth}x${viewportHeight}",
      "cd": "${screenColorDepth}",
      "t": "${t}",
      "k": "${k}",
      "usr": "${usr}",
      "prf": {
        "plt": "${pageLoadTime}",
        "dlt": "${domainLookupTime}",
        "tct": "${tcpConnectTime}",
        "srt": "${serverResponseTime}",
        "pdt": "${pageDownloadTime}",
        "rt": "${redirectTime}",
        "dit": "${domInteractiveTime}",
        "clt": "${contentLoadTime}"
      }
    }
  },
  "triggers": {
    "pageView": {
      "on": "visible",
      "request": "pageView"
    }
  },
  "transport": {
    "beacon": true,
    "useBody": true
  }
}
